{
	"name": "middleware-io",
	"version": "0.0.5",
	"description": "Modern middleware with promises and status",
	"main": "lib/index.js",
	"module": "lib/index.mjs",
	"scripts": {
		"rollup:build": "cross-env NODE_ENV=production rollup -c",
		"rollup:watch": "npm run rollup:build -- -w",
		"build": "npm run rollup:build",
		"lint": "eslint ./src --ext .mjs,.js",
		"lint:fix": "npm run lint -- --fix",
		"test": "mocha test/*.test.mjs --require ./test/runtime.mjs",
		"benchmark": "mocha test/*.benchmark.mjs --require ./test/runtime.mjs",
		"prepare": "npm run build && npm run test"
	},
	"engines": {
		"node": ">=8"
	},
	"files": [
		"lib/index.js",
		"lib/index.mjs"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/negezor/middleware-io.git"
	},
	"keywords": [
		"middleware",
		"ware",
		"promise",
		"async",
		"await",
		"modern",
		"es6",
		"es7",
		"node",
		"esm",
		"js"
	],
	"author": "Negezor <negezor@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/negezor/middleware-io/issues"
	},
	"homepage": "https://github.com/negezor/middleware-io#readme",
	"devDependencies": {
		"@babel/core": "^7.0.0-beta.44",
		"@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.44",
		"@babel/preset-env": "^7.0.0-beta.44",
		"@babel/register": "^7.0.0-beta.44",
		"benchmark": "^2.1.4",
		"chai": "^4.1.2",
		"cross-env": "^5.1.4",
		"eslint": "^4.19.1",
		"eslint-config-airbnb-base": "^12.1.0",
		"eslint-plugin-import": "^2.10.0",
		"mocha": "^5.0.5",
		"rollup": "^0.57.1",
		"rollup-plugin-babel": "^4.0.0-beta.4",
		"rollup-plugin-commonjs": "^9.1.0",
		"rollup-plugin-node-resolve": "^3.3.0"
	}
}
